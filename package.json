{
	"name": "addDocComments",
	"displayName": "Add jsdoc comments",
	"version": "0.0.1",
	"private": false,
	"description": "Adds jsdoc @param and @return tags for selected function signatures in JS and TS",
	"homepage": "https://github.com/CodeEditorLand/Foundation#readme",
	"bugs": {
		"url": "https://github.com/CodeEditorLand/Foundation/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/CodeEditorLand/Foundation.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Playform",
		"email": "Hello@Playform.Cloud",
		"url": "https://playform.cloud"
	},
	"type": "module",
	"main": "./out/extension",
	"scripts": {
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./",
		"prepublishOnly": "TypeScriptESBuild 'Source/**/*.ts'",
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.addDocComments",
				"title": "Add Doc Comments"
			}
		]
	},
	"activationEvents": [
		"onCommand:extension.addDocComments"
	],
	"dependencies": {
		"indent-string": "5.0.0"
	},
	"devDependencies": {
		"typescript-esbuild": "0.4.5",
		"vscode": "0.10.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"icon": "images/comment_logo.svg",
	"galleryBanner": {
		"color": "#5c2d91",
		"theme": "dark"
	}
}
